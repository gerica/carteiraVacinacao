{
    "extends": "rallycoding",
    "rules": {
        "max-len": [
            2,
            200,
            4,
            {
                "ignoreUrls": true,
                "ignoreComments": true,
                "ignoreTrailingComments": true,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true
            }
        ]
    }
}